    <form action="/search" method="get">
        <input type="text" name="q" placeholder="Search..." value="{{query}}">
        <button type="submit">Search</button>
    </form>

    {% if query and names != ''%}
        <p>Du s√∂kte efter {{query}}</p>
        <p>Vi hittade {{names}}</p>
    {% elseif query != false %}
        <p>Inga resultat</p>
    {%endif%}
    
</main>
{% endblock %}